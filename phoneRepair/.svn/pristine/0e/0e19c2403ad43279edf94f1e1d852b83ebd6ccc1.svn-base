				}
			//店铺主修
				if($("#smajor").val().trim()==""){
					$("#smajorTip").text("主修不能为空");
					$("#smajorTip").css({display:"block"});
					return false;
				}
			//8.描述
				if($("#sdesc").val().trim()==""){
					$("#sdescTip").text("描述不能为空");
					$("#sdescTip").css({display:"block"});
					return false;
				}
			//9.验证码
				if($("#code").val().length==0){
					$("#codeTip").text("验证码不能为空");
					$("#codeTip").css({display:"block"});
					return false;
				}
		});
	})

