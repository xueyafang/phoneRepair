<script type="text/javascript">
	$(function(){
		/*点击图片更换验证码*/
		//alert($("#code2").val());
		$("#code1").click(function(){
			$("#code1").attr({src:"/phoneRepair/code?id="+Math.random()});
		});
		/*校验*/
		//1.用户名为手机号,用户名不能重复
		$("#susername").blur(function(){
			var susername = $("#susername");
			if(!(/^1[3|4|5|7|8][0-9]{9}$/.test(susername.val()))){
				susername.val("");
				$("#susernameTip").text("请您输入正确的手机号");
				$("#susernameTip").css({display:"block"});
			}else{
				//去判断用户名唯一
				$.ajax({
					url:"/phoneRepair/store",
					data:{
						method:"isExistStore",
						susername:susername.val(),
					},
					error:function(){
					},
					success:function(data){
						//如果存在用户
						if(data==1){
							$("#susernameTip").text("该手机号已被注册");
							$("#susernameTip").css({display:"block"});
							$("#susername").val("");
						}
						//如果不存在该用户
						if(data==0){
							$("#susernameTip").text("");
							$("#susernameTip").css({display:"none"});
						}
					}
						
				}); 
			}
		});
		//2.密码不能少于6位
		$("#spassword").blur(function(){
			var spassword = $("#spassword");
			if(!(/^\w{6,}$/.test(spassword.val()))){
				$("#spasswordTip").text("密码不能小于6位");
				$("#spasswordTip").css({display:"block"});
			}else{
				$("#spasswordTip").text("");
				$("#spasswordTip").css({display:"none"});
			}
		});
		//3.确认密码
		$("#repwd").blur(function(){
			if($("#spassword").val()!=$("#repwd").val()){
				$("#repwdTip").text("两次密码不一致");
				$("#repwdTip").css({display:"block"});
			}else{
				$("#repwdTip").text("");
				$("#repwdTip").css({display:"none"});
			}
		});
		//4.店铺名不能为空
		$("#sname").blur(function(){
			if($("#sname").val().trim()==""){
				$("#snameTip").text("店铺名不能为空");
				$("#snameTip").css({display:"block"});
			}else{
				$("#snameTip").text("");
				$("#snameTip").css({display:"none"});
			}
		});
		//5.店铺地址
		//6.图片
		
		//7.营业时间
		$("#stime").blur(function(){
			if($("#stime").val().length==0){
				$("#stimeTip").text("店铺名不能为空");
				$("#stimeTip").css({display:"block"});
			}else{
				$("#stimeTip").text("");
				$("#stimeTip").css({display:"none"});
			}
		});
		
		//店铺主修
		$("#smajor").blur(function(){
			if($("#smajor").val().trim()==""){
				$("#smajorTip").text("主修不能为空");
				$("#smajorTip").css({display:"block"});
			}else{
				$("#smajorTip").text("");
				$("#smajorTip").css({display:"none"});
			}
		});
		//8.描述
		$("#sdesc").blur(function(){
			if($("#sdesc").val().trim()==""){
				$("#sdescTip").text("描述不能为空");
				$("#sdescTip").css({display:"block"});
			}else{
				$("#sdescTip").text("");
				$("#sdescTip").css({display:"none"});
			}
		});
		//9.验证码
		$("#code").mouseout(function(){
			$.ajax({
				url:"/phoneRepair/store",
				data: "method=getCode&msg="+msg,
				error:function(){
				},
				success:function(data){
					if(data==1){
				    	 $("#codeTip").text("");
				     }else if(data==0){
				    	 $("#codeTip").text("验证码错误!");
				     }
				}
					
			}); 
			
		});
		//表单验证
		$("#form").submit(function(){
			//alert("111");
				var susername = $("#susername");
				if(!(/^1[3|4|5|7|8][0-9]{9}$/.test(susername.val()))){
					susername.val("");
					$("#susernameTip").text("请您输入正确的手机号");
					$("#susernameTip").css({display:"block"});
					return false;
				}
			//2.密码不能少于6位
				var spassword = $("#spassword");
				if(!(/^\w{6,}$/.test(spassword.val()))){
					$("#spasswordTip").text("密码不能小于6位");
					$("#spasswordTip").css({display:"block"});
					return false;
				}
				
			//3.确认密码
				var repwd = $("#repwd");
				if($("#spassword").val()!=$("#repwd").val()){
					$("#repwdTip").text("两次密码不一致");
					$("#repwdTip").css({display:"block"});
					return false;
				}
			//4.店铺名不能为空
				if($("#sname").val().trim()==""){
					$("#snameTip").text("店铺名不能为空");
					$("#snameTip").css({display:"block"});
					return false;
				}
			//5.店铺地址
			//6.图片
			
			//7.营业时间
				if($("#stime").val().trim()==""){
					$("#stimeTip").text("店铺名不能为空");
					$("#stimeTip").css({display:"block"});
