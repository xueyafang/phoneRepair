.carousel{width:320px;position:relative;overflow:hidden;z-index:1;height:240px}.carousel ol,.carousel li{margin:0;padding:0;list-style:none}.carousel .carousel-indicators{position:absolute;height:12px;bottom:20px;z-index:60}.carousel .carousel-indicators li{width:12px;height:12px;border:1px solid #fff;-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;margin:0 2px;float:left;cursor:pointer;background-color:#fff}.carousel .carousel-indicators li.active{width:14px;height:14px;border:0;background-color:#d73c2d}.carousel .carousel-caption{position:absolute;color:#fff;bottom:47px;text-align:center;width:100%;line-height:150%}.carousel.slide .carousel-inner{position:absolute;left:0}.carousel.slide .carousel-inner .item{float:left}.carousel.slide .carousel-inner .item.active{display:block}.carousel.fade .carousel-inner{position:relative;left:0}.carousel.fade .carousel-inner .item{position:absolute;top:0;left:0;z-index:10}.carousel.fade .carousel-inner .item.active{z-index:40}.carousel.fade .carousel-inner .item.prepared{z-index:20}.carousel .left,.carousel .right{position:absolute;top:0;cursor:pointer;width:50px;height:100%}.carousel .left{left:0;z-index:100}.carousel .right{right:0;z-index:100}