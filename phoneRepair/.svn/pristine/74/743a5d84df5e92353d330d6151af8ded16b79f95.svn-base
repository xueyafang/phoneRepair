<script type="text/javascript">
$(function(){
	$.ajax({
		url:"/phoneRepair/store",
		data: "method=show",
		dataType:"json",
		error:function(){
			alert("ajax出错");
		},
		success:function(data){
			 $.each(data,function(i,dad){
				 //控制为6条数据 
				$("#storeList").append("<li class='clearfix first'><div class='small'><a href='#'><img src='"+dad.spic+"' width='93' alt='"+dad.sname+"'></a></div><div class='pic_bd'><p class='store_name'><a href='#'>"+dad.sname+"</a> <b><img src='../img/index/rankicon1.png' width='16' title='1级' alt='1级'></b></p><p><span class='Authentication id_icon'>认证</span></p><p><span class='address_detial'>地址:"+dad.saddress+"</span><span class='map_icon' data-deg='116.412156,39.924616'></span></p></div></li>");
			}); 
		}
		
	});
	//一、通过ajax获取数据库中的省市
	//1.加载省份
	var provinceArr = new Array();
	$.ajax({
		url:"/phoneRepair/city",
		data: "method=getProvince",
		error:function(){
		},
		success:function(data){
			provinceArr = data.split(",");
			for(var i=0;i<provinceArr.length;i++){
				//alert(provinceArr[i]);
				var $option = $("<option>"+provinceArr[i]+"</option>");
				$("#province").append($option);
			}
		}
			
	}); 
	var cityArr = new Array();
	//2.加载城市
	$("#province").change(function(){
		$("#city")[0].length = 1;
		var index = this.selectedIndex;
		$.ajax({
			url:"/phoneRepair/city",
			data: "method=getCity&provinceId="+index,
			error:function(){
			},
			success:function(data){
				cityArr = data.split(",");
				for(var j=0;j<cityArr.length;j++){
					var $option = $("<option>"+cityArr[j]+"</option>");
					$("#city").append($option);
				}
			}
			
		});
		
	});
	//二.省市改变的时候去获取数据
	//1.省份改变
	$("#province").change(function(){
		$("#storeList").empty();
		var province = $("#province").val();
		$.ajax({
			url:"/phoneRepair/store",
			data: "method=getStore&searchAddress="+province,
			dataType:"json",
			error:function(){
				alert("ajax出错");
			},
			success:function(data){
				 $.each(data,function(i,dad){
					 //控制为6条数据
					 if(i<6){
						 $("#storeList").append("<li class='clearfix first'><div class='small'><a href='#'><img src='"+dad.spic+"' width='93' alt='"+dad.sname+"'></a></div><div class='pic_bd'><p class='store_name'><a href='#'>"+dad.sname+"</a> <b><img src='../img/index/rankicon1.png' width='16' title='1级' alt='1级'></b></p><p><span class='Authentication id_icon'>认证</span></p><p><span class='address_detial'>地址:"+dad.saddress+"</span><span class='map_icon' data-deg='116.412156,39.924616'></span></p></div></li>");
					 }
				}); 
			}
			
		});
	});
	//2.城市改变
	$("#city").change(function(){
		$("#storeList").empty();
		var city = $("#city").val();
		$.ajax({
			url:"/phoneRepair/store",
			data: "method=getStore&searchAddress="+city,
			dataType:"json",
			error:function(){
				alert("ajax出错");
			},
			success:function(data){
				 $.each(data,function(i,dad){
					 //控制为6条数据
					 if(i<6){
						 $("#storeList").append("<li class='clearfix first'><div class='small'><a href='#'><img src='"+dad.spic+"' width='93' alt='"+dad.sname+"'></a></div><div class='pic_bd'><p class='store_name'><a href='#'>"+dad.sname+"</a> <b><img src='../img/index/rankicon1.png' width='16' title='1级' alt='1级'></b></p><p><span class='Authentication id_icon'>认证</span></p><p><span class='address_detial'>地址:"+dad.saddress+"</span><span class='map_icon' data-deg='116.412156,39.924616'></span></p></div></li>");
					 }
				}); 
			}
			
		});
	});
});
</script>
