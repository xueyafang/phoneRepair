/*banner*/
.slideBox{
    width: 1920px;
    float: right;
    position: relative;
    height: 430px;
    margin-left: auto;
    margin-right: auto;
}
.slideBox .hd{
    height: 15px;
    overflow: hidden;
    position: absolute;
    left: 50%;
    margin-left: -80px;
    bottom: 10px;
    z-index: 1;
}
.slideBox .hd ul{
    overflow:hidden; 
    zoom:1;
    float:left;  
}
.slideBox .hd ul li{
    float: left;
    margin: 0 15px;
    width: 8px;
    height: 8px;
    border: 1px solid #40bb8c;
    cursor: pointer;
    border-radius: 50%;
}
.slideBox .hd ul li.on{
 border-width:3px;
 width: 4px;
 height: 4px;
}
.slideBox .bd{
     position:relative; 
     height:430px; 
     z-index:0; 
	 overflow:hidden;
}
.slideBox .bd ul{
     margin: 0;
}
.slideBox .bd li{ 
    zoom:1; 
    vertical-align:middle; 
	height:430px;
}
.slideBox .bd li a {
    display: inline-block;
    overflow: hidden;
    text-align: center;
    width: 1920px;
	height:430px;
}
.slideBox .bd li a span{
    display: block;
}
.slideBox .bd ul li img {
    width: auto;
    height: 100%;
    display: inline-block;
}

